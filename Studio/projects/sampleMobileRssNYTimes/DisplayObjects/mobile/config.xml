    <gap:splash gap:density="hdpi" gap:platform="android" src="res/splash/android/hdpi.png"/>
    <gap:splash gap:density="xhdpi" gap:platform="android" src="res/splash/android/xhdpi.png"/>
    <gap:splash gap:platform="blackberry" src="res/splash/bb/splash225.png"/>
    <gap:splash gap:platform="ios" height="480" src="res/splash/ios/splashiPhone.png" width="320"/>
    <gap:splash gap:platform="ios" height="320" src="res/splash/ios/splashiPhoneLandscape.png" width="480"/>
    <gap:splash gap:platform="ios" height="960" src="res/splash/ios/splashiPhone2x.png" width="640"/>
    <gap:splash gap:platform="ios" height="640" src="res/splash/ios/splashiPhoneLandscape2x.png" width="960"/>
