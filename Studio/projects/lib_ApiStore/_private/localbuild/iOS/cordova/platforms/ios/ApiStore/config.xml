<?xml version='1.0' encoding='utf-8'?>
<widget id="com.convertigo.mobile.ApiStore" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <icon height="180" src="www/res/icon/icon603x.png" width="180" />
    <icon height="120" src="www/res/icon/icon602x.png" width="120" />
    <icon height="57" src="www/res/icon/icon57.png" width="57" />
    <icon height="72" src="www/res/icon/icon72.png" width="72" />
    <icon height="114" src="www/res/icon/icon572x.png" width="114" />
    <icon height="144" src="www/res/icon/icon722x.png" width="144" />
    <splash height="2208" src="www/res/splash/Default736hiphone.png" width="1242" />
    <splash height="1334" src="www/res/splash/Default667hiphone.png" width="750" />
    <splash height="1136" src="www/res/splash/Default568h2xiphone.png" width="640" />
    <splash height="1496" src="www/res/splash/DefaultLandscape2xipad.png" width="2048" />
    <splash height="748" src="www/res/splash/DefaultLandscapeipad.png" width="1024" />
    <splash height="2008" src="www/res/splash/DefaultPortrait2xipad.png" width="1536" />
    <splash height="1004" src="www/res/splash/DefaultPortraitipad.png" width="768" />
    <splash height="960" src="www/res/splash/Default2xiphone.png" width="640" />
    <splash height="480" src="www/res/splash/Defaultiphone.png" width="320" />
    <name>ApiStore</name>
    <description />
    <gap:config-file parent="CFBundleShortVersionString" platform="ios">
        <string>0.0.1</string>
    </gap:config-file>
    <author email="sales@convertigo.com" href="http://www.convertigo.com">Convertigo</author>
    <icon src="www/icon.png" />
    <splash src="www/splash.png" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <preference name="phonegap-version" value="cli-5.4.1" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="prerendered-icon" value="true" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="permissions" value="none" />
</widget>
