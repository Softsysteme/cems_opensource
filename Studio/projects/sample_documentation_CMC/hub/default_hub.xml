	<origin name="ListOfArticles">
		<event name="event_articleClicked">
			<target name="GoogleClip">
				<mapping><![CDATA[
					return {
						// action
						__transaction : "SearchGoogleClip",
						
						// parameters
						keyword : payload.name
					}
				]]></mapping>
			</target>
		</event>
	</origin>

</hub>